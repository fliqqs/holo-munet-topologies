interfaces interface lo
 type iana-if-type:ethernetCsmacd
 !
 ipv6 address 2001:db8:1000::2
  prefix-length 128
 !
!
interfaces interface eth-rt1
 type iana-if-type:ethernetCsmacd
 ipv6
!
interfaces interface eth-rt3
 type iana-if-type:ethernetCsmacd
 ipv6
!
interfaces interface eth-rt4
 type iana-if-type:ethernetCsmacd
 ipv6
!
routing control-plane-protocols control-plane-protocol ietf-ospf:ospfv3 main
 ospf explicit-router-id 2.2.2.2
 !
 ospf areas area 0.0.0.0
  interfaces interface lo
  !
  interfaces interface eth-rt1
   interface-type point-to-point
   hello-interval 3
   dead-interval 12
  !
  virtual-links virtual-link 0.0.0.1 5.5.5.5
  !
  virtual-links virtual-link 0.0.0.2 5.5.5.5
  !
 !
 ospf areas area 0.0.0.1
  interfaces interface eth-rt3
   interface-type point-to-point
   hello-interval 3
   dead-interval 12
  !
 !
 ospf areas area 0.0.0.2
  interfaces interface eth-rt4
   interface-type point-to-point
   hello-interval 3
   dead-interval 12
  !
 !
!
